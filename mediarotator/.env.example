# Example environment file for MediaRotator
# Copy to .env and fill values before running.

# Required
RADARR_API_KEY=
SONARR_API_KEY=

# Optional
RADARR_URL=http://localhost:7878
SONARR_URL=http://localhost:8989
RADARR_QUALITY_PROFILE_ID=1
SONARR_QUALITY_PROFILE_ID=1
SONARR_LANGUAGE_PROFILE_ID=1
MOVIE_ROOT_FOLDER=/mnt/netstorage/Media/RotatingMovies
SHOW_ROOT_FOLDER=/mnt/netstorage/Media/RotatingTV
ENABLE_NOTIFICATIONS=true
MEDIA_CHANGE_FILE=last_media_change.txt
